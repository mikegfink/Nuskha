<div id="container">
</div>
<script>
    var json = buildSigmaGraph(nuskhaExpression);
    s = new sigma({
        graph: json,
        container: 'container',
        renderer: {
            container: document.getElementById('container'),
            type: 'canvas'
        },
        settings: {
            edgeColor: 'default',
            defaultNodeColor: '#065a8f',
            minNodeSize: 8,
            maxNodeSize: 8
        }
    });

    // Generating random y cordinates. TODO: Eliminate duplication
    nodes = [];
    nodes = s.graph.nodes();
    len = nodes.length;
    for (i = 0; i < len; i++) {
        nodes[i].y = Math.random();
        nodes[i].size = s.graph.degree(nodes[i].id);
    }

    s.refresh();
    s.startForceAtlas2();
    s.start
</script>